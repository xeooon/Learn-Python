str:
	upper()	        全大写
	lower()	        全小写
	capitalize()	首字母大写,其他字母小写
	title()	    每个单词字母大写
	swapcase()	大小写翻转
	isalnum()	字符串由字母或数字组成
	isalpha()	字符串由字母组成
	isdigit()	字符串由数字组成
	strip()	    去除首尾空格、换行符、制表符或指定字符
	center()	居中,可以有填充物
	replace()	替换
	startswith()	以什么开头
	endswith()	    以什么结尾
	find()	        通过元素找索引,找不到返回-1,可自己定义返回值
	index()	通过元素找索引,找不到会报错
	count()	计算元素的个数,可以切片
	split()	指定元素分割,str转换成list
	join()	list(每个元素必须是str)转换str
	len()	测量长度, 个数
	format()	格式化输出

list:
	append()	追加
	insert()	插入,指定位置
	extend()	迭代追加
	pop()	    按照索引或下标删除,有返回值
	remove()	按照元素删除
	del()	    按照索引删除或按照切片删除
	clear	    清空列表
	li[1] = 666	            通过索引去改
	li[1:4] = 666	        通过切片去改
	print(li[1])	        索引查找
	print(li[1:5:2])	    切片查找
	for i in li: print(i)	for循环查找

dict:
	setdefault()	        有则不变,否则添加
	dic['key'] = 'value'	有则替换,无则增加
	pop()	    按照key删除,回值删除的值
	popitem()	随机删除
	clear	    清空字典
	del()	    按照key去删除
	update()	覆盖增加
	get()	    通过key去查
	keys()	    查字典的key
	values()	查字典的value
	items()	    查字典的key和value
